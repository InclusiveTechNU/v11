{
  "name": "v11",
  "package-name": "v11",
  "main": "./build/v11/core/v11.js",
  "version": "0.4.1",
  "dependencies": {
    "@types/node": "^14.0.1",
    "get-uv-event-loop-napi-h": "^1.0.6",
    "gts": "^2.0.2",
    "node-gyp": "^6.1.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.2"
  },
  "scripts": {
    "check": "./third_party/node/node_modules/gts/build/src/cli.js check",
    "clean": "rm -rf ./larkin/build && rm -rf ./build && yarn run clean:js",
    "clean:js": "./third_party/node/node_modules/gts/build/src/cli.js clean",
    "fix": "./third_party/node/node_modules/gts/build/src/cli.js fix",
    "build": "yarn install && ./tools/build_larkin_client && yarn run clean:js && yarn run check && ./third_party/node/node_modules/typescript/bin/tsc",
    "preinstall": "$npm_execpath --silent run checkyarn",
    "checkyarn": "echo \"$npm_execpath\" | grep -q \"yarn\\.js$\" || (echo '⚠️  Use yarn not npm! ⚠️ ' && echo && exit 1)"
  }
}
